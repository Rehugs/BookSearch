{
  "name": "book-searcher",
  "buildCommand": "vite build -m vercel",
  "devCommand": "vite dev -m vercel --port $PORT",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/search",
      "destination": "https://book-searcher.eu.org/search"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    }
  ]
}
